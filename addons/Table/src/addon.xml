<?xml version='1.0' encoding='UTF-8' ?>

<addon id='Table' xmlns="http://icplayer.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://icplayer.com ../../tools/addon.xsd">
	<model>
		<property name='Rows' type='string' />
		<property name="Columns" type="string" />
		<property name="Table cells" type="list">
			<property name="Row" type="string" />
			<property name="Column" type="string" />
			<property name="Content" type="html" isLocalized="true" />
		</property>
		<property name="Columns width" type="list">
			<property name="Width" type="string" />
		</property>
		<property name="Rows height" type="list">
			<property name="Height" type="string" />
		</property>
		<property name="Is not an activity" type="boolean" />
		<property name="Is disabled" type="boolean" />
		<property name="Case sensitive" type="boolean" />
		<property name="Ignore punctuation" type="boolean" />
		<property name="Gap width" type="string" />
	</model>
</addon>