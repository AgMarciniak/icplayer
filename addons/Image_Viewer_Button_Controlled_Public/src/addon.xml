<?xml version='1.0' encoding='UTF-8' ?>

<addon id='Image_Viewer_Button_Controlled_Public' xmlns="http://icplayer.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://icplayer.com ../../tools/addon.xsd">
	<model>
		<property name='Image' type='file' />
		<property name="Frames" type="string" />
		<property name="Frame size" type="{Original, Keep aspect ratio, Stretch}" />
		<property name="Sounds" type="list">
			<property name="MP3 sound" type="file" />
			<property name="OGG sound" type="file" />
			<property name="AAC sound" type="file" />
		</property>
		<property name="Frame names" type="list">
			<property name="name" type="string" />
			<property name="frame" type="string" />
		</property>
		<property name="isClickDisabled" type="boolean" />
		<property name="Labels" type="list">
			<property name="Text" type="string" isLocalized="true" />
			<property name="Top" type="string" />
			<property name="Left" type="string" />
			<property name="Frames" type="string" />
		</property>
		<property name="Show frame" type="string" />
	</model>
</addon>