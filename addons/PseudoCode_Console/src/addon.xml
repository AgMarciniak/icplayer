<?xml version='1.0' encoding='UTF-8' ?>

<addon id='PseudoCode_Console' xmlns="http://icplayer.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://icplayer.com ../../tools/addon.xsd">
