<?xml version='1.0' encoding='UTF-8' ?>

<addon id='Slider'>
	<model>
		<property name='ImageElement' type='image' />
		<property name="Orientation" type="{Landscape, Portrait}" />
		<property name="Stepwise" type="boolean" />
		<property name="StepsCount" type="string" />
		<property name="InitialStep" type="string" />
		<property name="onStepChange" type="event" />
		<property name="Block in error checking mode" type="boolean" />
	</model>
</addon>